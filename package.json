    "react-icons": "4.8.0",
    "react-is": "18.2.0",
    "react-number-format": "5.3.0",
    "react-select": "5.7.4",
    "react-table": "7.8.0",
    "rimraf": "5.0.1",
    "stylis": "4.2.0",
    "stylis-plugin-rtl": "2.1.1",
    "typescript": "5.0.4",
    "web-vitals": "3.3.1",
    "zod": "3.21.4",
    "zustand": "4.3.8"
