{
  "name": "nextjs-chakra-ui-skeleton",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV='development' next dev",
    "build": "next build && next export",
    "start": "next start",
    "lint": "next lint",
    "lint-fix": "next lint --fix",
    "clean-build": "rimraf build",
